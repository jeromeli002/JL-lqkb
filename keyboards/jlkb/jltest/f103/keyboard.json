{
    "keyboard_name": "分体测试",
    "manufacturer": "JL",
    "url": "http://wpa.qq.com/msgrd?v=3&uin=532192607&site=qq&menu=yes",
    "maintainer": "qmk",
    "usb": {
        "vid": "0xF1ED",
        "pid": "0x0B91",
        "device_version": "0.0.1"
    },
    "encoder": {
    "rotary": [
      { "pin_a": "B14", "pin_b": "B15", "resolution": 4 },
      { "pin_a": "B12", "pin_b": "B13", "resolution": 4 }
    ]
  }
    "split": {
        "transport": {
            "sync": {
                "matrix_state": true
            }
        }
    }, 
    "rgblight": {
        "saturation_steps": 8,/* 饱和度步数 */
        "brightness_steps": 8,/* 亮度步数 */
        "led_count": 8, /* 灯数量 */
        "hue_steps":8,/* 色调步数 */
        "max_brightness":250,/* 最大亮度 */
        "sleep": true,/* 睡眠时关闭灯 */
        /* "led_map": [4, 3, 2, 1, 0], LE灯D顺序 */
        "animations": {/* 灯效 */
            "static_light": true,
            "breathing": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "snake": true,
            "knight": true,
            "christmas": true,
            "static_gradient": true,
            "rgb_test": true,
            "alternating": true,
            "twinkle": true
        }
    },
     "ws2812": {
        "pin": "A0"
    },
    "matrix_pins": {
        "cols": ["C13", "C14", "C15", null, null],
        "rows": ["B9", "B8", null, null, "NO_PIN"]
    },
    "diode_direction": "COL2ROW",

    "layouts": {
        "LAYOUT": {
            "layout": [
            { "matrix": [0, 0],"x": 0,"y": 0}, 
