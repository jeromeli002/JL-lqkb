    "usb": {
        "vid": "0xF1ED",
        "pid": "0x0B91",
        "device_version": "0.0.1"
    },
    "encoder": {
    "rotary": [
      { "pin_a": "B9", "pin_b": "C10", "resolution": 4 },
      { "pin_a": "B9", "pin_b": "C11", "resolution": 4 }
    ]
  }
  
    "rgblight": {
        "saturation_steps": 8,/* 饱和度步数 */
        "brightness_steps": 8,/* 亮度步数 */
        "led_count": 28, /* 灯数量 */
        "hue_steps":8,/* 色调步数 */
        "max_brightness":250,/* 最大亮度 */
        "sleep": true,/* 睡眠时关闭灯 */
        /* "led_map": [4, 3, 2, 1, 0], LE灯D顺序 */
        "animations": {/* 灯效 */
            "static_light": true,
            "breathing": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "snake": true,
            "knight": true,
            "christmas": true,
            "static_gradient": true,
            "rgb_test": true,
            "alternating": true,
            "twinkle": true
        }
    },
