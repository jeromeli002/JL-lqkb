{
    "keyboard_name": "JL-控制器A",
    "manufacturer": "JL",
    "url": "http://wpa.qq.com/msgrd?v=3&uin=532192607&site=qq&menu=yes",
    "maintainer": "qmk",
    "usb": {
        "vid": "0xF1ED",
        "pid": "0x0B91",
        "device_version": "0.0.1"
    },
    "encoder": {
    "rotary": [
      { "pin_a": "B14", "pin_b": "B13", "resolution": 4 },
      { "pin_a": "A8", "pin_b": "B15", "resolution": 4 },
      { "pin_a": "B4", "pin_b": "B3", "resolution": 4 }
    ]
  }
  
    "rgblight": {
        "saturation_steps": 8,/* 饱和度步数 */
        "brightness_steps": 8,/* 亮度步数 */
        "led_count": 3, /* 灯数量 */
        "hue_steps":8,/* 色调步数 */
        "max_brightness":128,/* 最大亮度 */
        "sleep": true,/* 睡眠时关闭灯 */
        /* "led_map": [4, 3, 2, 1, 0], LE灯D顺序 */
        "animations": {/* 灯效 */
            "static_light": true,
            "breathing": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "snake": true,
            "knight": true,
            "christmas": true,
            "static_gradient": true,
            "rgb_test": true,
            "alternating": true,
            "twinkle": true
        }
    },
     "ws2812": {
        "pin": "B12"
    },
    "matrix_pins": {
        "cols": ["A4", "A5", "A6"],
        "rows": ["B0", "B1", "B10"]
    },
    "diode_direction": "COL2ROW",

    "layouts": {
        "LAYOUT": {
            "layout": [
            { "matrix": [0, 0],"x": 0,"y": 0}, 
            { "matrix": [0, 1],"x": 1,"y": 0},
            { "matrix": [0, 2],"x": 2,"y": 0},
             
            { "matrix": [1, 0],"x": 0,"y": 1}, 
            { "matrix": [1, 1],"x": 1,"y": 1}, 
            { "matrix": [1, 2],"x": 2,"y": 1}, 
            
            { "matrix": [2, 0],"x": 0,"y": 2}, 
            { "matrix": [2, 1],"x": 1,"y": 2}, 
            { "matrix": [2, 2],"x": 2,"y": 2}
			]
        }
    }
}